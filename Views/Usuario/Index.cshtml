@model IEnumerable<Usuario>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NmUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DsEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DsSenha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DsTipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DsLocalizacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DsFotoPerfil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pescaria)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NmUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DsEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DsSenha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DsTipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DsLocalizacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DsFotoPerfil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pescaria.CdPescaria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CdUsuario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CdUsuario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CdUsuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
